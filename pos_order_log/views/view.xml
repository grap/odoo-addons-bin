<openerp><data>

    <record id="view_pos_order_log_search" model="ir.ui.view">
        <field name="model">pos.order.log</field>
        <field name="arch" type="xml">
            <search string="Pos Order Log">
                <field name="user_id"/>
                <field name="date"/>
                <field name="name"/>
            </search>
        </field>
    </record>

    <record id="view_pos_order_log_tree" model="ir.ui.view">
        <field name="model">pos.order.log</field>
        <field name="arch" type="xml">
            <tree string="Pos Order Log">
                <field name="user_id"/>
                <field name="date"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_pos_order_log_form" model="ir.ui.view">
        <field name="model">pos.order.log</field>
        <field name="arch" type="xml">
            <form string="Pos Order Log">
                <field name="user_id"/>
                <field name="date"/>
                <field name="name"/>
                <field name="note"/>
            </form>
        </field>
    </record>

</data></openerp>
